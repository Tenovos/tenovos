{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "allowJs": false,
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "declaration": true,
    "sourceMap": true,
    "baseUrl": "./apps/web-ui",
    "types": [
      "webpack-env",
      "node",
      "jest"
    ],
    "paths": {
      "@/*": ["./*"],
      "@": [
        "./index.ts",
        "./index.json",
        "./index.vue",
        "./index.scss",
        "./index.css"
      ],
      "@src/*": ["src/*"],
      "@src": [
        "src/index.ts",
        "src/index.json",
        "src/index.vue",
        "src/index.scss",
        "src/index.css"
      ],
      "@router/*": ["src/router/*"],
      "@router": [
        "src/router/index.ts",
        "src/router/index.json",
        "src/router/index.vue",
        "src/router/index.scss",
        "src/router/index.css"
      ],
      "@pages/*": ["src/router/pages/*"],
      "@pages": [
        "src/router/pages/index.ts",
        "src/router/pages/index.json",
        "src/router/pages/index.vue",
        "src/router/pages/index.scss",
        "src/router/pages/index.css"
      ],
      "@layouts/*": ["src/router/layouts/*"],
      "@layouts": [
        "src/router/layouts/index.ts",
        "src/router/layouts/index.json",
        "src/router/layouts/index.vue",
        "src/router/layouts/index.scss",
        "src/router/layouts/index.css"
      ],
      "@components/*": ["src/components/*"],
      "@components": [
        "src/components/index.ts",
        "src/components/index.json",
        "src/components/index.vue",
        "src/components/index.scss",
        "src/components/index.css"
      ],
      "@assets/*": ["src/assets/*"],
      "@assets": [
        "src/assets/index.ts",
        "src/assets/index.json",
        "src/assets/index.vue",
        "src/assets/index.scss",
        "src/assets/index.css"
      ],
      "@utils/*": ["src/utils/*"],
      "@utils": [
        "src/utils/index.ts",
        "src/utils/index.json",
        "src/utils/index.vue",
        "src/utils/index.scss",
        "src/utils/index.css"
      ],
      "@plugins/*": ["src/plugins/*"],
      "@plugins": [
        "src/plugins/index.ts",
        "src/plugins/index.json",
        "src/plugins/index.vue",
        "src/plugins/index.scss",
        "src/plugins/index.css"
      ],
      "@state/*": ["src/state/*"],
      "@state": [
        "src/state/index.ts",
        "src/state/index.json",
        "src/state/index.vue",
        "src/state/index.scss",
        "src/state/index.css"
      ],
      "@typings/*": ["src/typings/*"],
      "@typings": [
        "src/typings/index.ts",
        "src/typings/index.json",
        "src/typings/index.vue",
        "src/typings/index.scss",
        "src/typings/index.css"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost",
    ]
  },
  "exclude": [
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/test.config.ts",
    "node_modules",
    "**/build/*",
    "**/*.d.ts"
  ]
}
